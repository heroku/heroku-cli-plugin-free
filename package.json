{
  "name": "@heroku-cli/plugin-free",
  "description": "A heroku cli plugin to find your free dynos and data",
  "version": "0.1.3",
  "author": "Terence Lee @hone",
  "bugs": "https://github.com/heroku/heroku-cli-plugin-free/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^8.5.0",
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "cli-ux": "^6.0.9",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/plugin-help": "^3.3.1",
    "@oclif/test": "^1.2.9",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1.2",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-plugin-free",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-plugin-free",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
